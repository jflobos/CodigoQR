<!DOCTYPE html>
<html>
<head>
    <title>Qr-Invitaciones</title>
    <link href="@Url.Content("~/css/bootstrap.min.css")" rel="stylesheet" media="screen" type="text/css" />
</head>
<body>
    <script src="http://code.jquery.com/jquery-latest.js" type="text/javascript"></script>
    <script src="@Url.Content("~/js/bootstrap.min.js")" type="text/javascript"></script>
    <div class="navbar navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container">
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse"><span
                    class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
                </a><a class="brand" href="../">Invitaciones QR</a>
                <div class="nav-collapse" id="main-menu">
                    <ul class="nav" id="main-menu-left">
                        <li><a href="@Url.Action("Eventos", "Home")">Eventos</a></li>
                        <li><a id="swatch-link" href="@Url.Action("Invitados", "Home")">Invitados</a></li>
                        <li><a id="swatch-linkb" href="@Url.Action("EnviarInvitaciones", "Home")">Envio Invitaciones</a></li>
                        <li class="dropdown"><a class="dropdown-toggle" data-toggle="dropdown" href="#">Estadisticas
                            <b class="caret"></b></a>
                            <ul class="dropdown-menu" id="swatch-menu">
                                <li><a href="@Url.Action("Estadisticas", "Home")">Estadisticas Personas</a></li>
                                <li class="divider"></li>
                                <li><a href="@Url.Action("Estadisticas", "Home")">Estadisticas Eventos</a></li>
                                
                            </ul>
                        </li>
                        <li class="dropdown" id="preview-menu"><a class="dropdown-toggle" data-toggle="dropdown"
                            href="#">Información <b class="caret"></b></a>
                            <ul class="dropdown-menu">
                                <li><a href="@Url.Action("Informacion", "Home")">Eventos Realizados</a></li>
                                <li><a href="@Url.Action("Informacion", "Home")">Eventos No realizados</a></li>
                                <li class="divider"></li>
                                <li><a href="@Url.Action("Informacion", "Home")">Invitados</a></li>
                             </ul>
                        </li>
                    </ul>
                    <ul class="nav pull-right" id="main-menu-right">
                        <li>@Html.Partial("_LogOnPartial")</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    @RenderBody()
</body>
</html>



   